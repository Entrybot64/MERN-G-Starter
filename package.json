{
	"name": "mern-g-starter",
	"version": "1.0.0",
	"description": "A starter for the MERN stack with an implementation of GraphQL with Apollo",
	"main": "index.js",
	"scripts": {
		"start-api": "nodemon api/src/app.js --exec babel-node --config-gile api/.babelrc",
		"start-app": "cd app && npm start",
		"start": "concurrently --names 'Client','Server' -c 'blue,green' \"npm run start-app\" \"npm run start-api\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Entrybot64/G-MERN-Stack-Starter.git"
	},
	"author": "",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Entrybot64/G-MERN-Stack-Starter/issues"
	},
	"homepage": "https://github.com/Entrybot64/G-MERN-Stack-Starter#readme",
	"devDependencies": {
		"@babel/cli": "^7.2.3",
		"@babel/core": "^7.2.2",
		"@babel/node": "^7.2.2",
		"@babel/preset-env": "^7.3.1",
		"@babel/register": "^7.0.0",
		"body-parser": "^1.18.3",
		"graphql": "^14.1.1",
		"graphql-tools": "^4.0.4",
		"nodemon": "^1.18.9"
	},
	"dependencies": {
		"concurrently": "^4.1.0",
		"cors": "^2.8.5",
		"dotenv": "^6.2.0",
		"express": "^4.16.4",
		"express-graphql": "^0.7.1",
		"lodash": "^4.17.11",
		"mongoose": "^5.4.7"
	}
}
